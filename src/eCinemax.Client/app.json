{
  "expo": {
    "name": "eCinemax.Client",
    "slug": "eCinemax",
    "version": "1.0.0",
    "scheme": "ecinemax.scheme",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/shared/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/shared/assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-localization"]
  }
}
